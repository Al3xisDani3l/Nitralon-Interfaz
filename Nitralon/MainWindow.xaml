                                <Grid.RowDefinitions>
                                    <RowDefinition Height="250"/>
                                    <RowDefinition Height="380"/>
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="920"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>

